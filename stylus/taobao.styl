	position relative
	margin 0 auto 
	.nav.left
		position absolute
		float left
		text-align center
		>div
			height 40px
			float left
			>a
				display inline-block
				height 20px
				margin-top 10px
				line-height 20px
			&:not(:last-of-type)>a
				border-right 1px solid gray
				height 20px
